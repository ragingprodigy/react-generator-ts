{
	"name": "react-generator-ts",
	"displayName": "react-generator-ts",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:react-generator-ts.generateComponent"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-generator-ts.generateComponent",
				"title": "React Generator: Generate Component"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "react-generator-ts.generateComponent",
					"group": "reactGeneratorGroup@1",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/change-case": "^2.3.1",
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.152",
		"@types/mkdirp": "^1.0.0",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"change-case": "^4.1.1",
		"lodash": "^4.17.15",
		"mkdirp": "^1.0.4"
	}
}
